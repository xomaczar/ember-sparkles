<rect fill="transparent" width="{{width}}" height="{{height}}"></rect>
{{yield (hash
  xPosition=xPosition
  xValue=xValue
  isActive=isActive
  isInactive=isInactive
  content=(component 'e-s/tooltip-content'
    xCenter=xPosition
    width=width
    maxWidth=width
    height=height
    xScale=xScale)
  )}}
